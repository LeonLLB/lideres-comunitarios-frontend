{
  "name": "lideres-comunitarios-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn tw:dev\" \"yarn vite:dev\"",
    "vite:dev": "vite dev",
    "tw:dev": "tailwindcss -i ./src/raw-tw.css -o ./src/tw.css -w",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "notiflix": "^3.2.6",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
